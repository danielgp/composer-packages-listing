{
    "name": "danielgp/composer-packages-listing",
    "description": "Wrapper for the composer.lock files attempting to normalize and enhance content included",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Popiniuc",
            "email": "danielpopiniuc@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0 || ^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "phpunit/php-code-coverage": "^4.0"
    },
    "autoload": {
        "classmap": [
            "ComposerPackagesListing.php"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "archive": {
        "exclude": ["/nbproject", "/tests"]
    }
}
