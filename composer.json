{
    "name": "danielgp/composer-packages-listing",
    "description": "Wrapper for the composer.lock files attempting to normalize and enhance content included",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Popiniuc",
            "email": "danielpopiniuc@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2 || ^7.3"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "~0.4",
        "phpunit/phpunit": "^7.2",
        "phpunit/php-code-coverage": "^6",
        "squizlabs/php_codesniffer": "^3.3",
        "wimg/php-compatibility": "~8.1"
    },
    "autoload": {
        "classmap": [
            "source/Basics.php",
            "source/ComposerPackagesListing.php"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "archive": {
        "exclude": ["/nbproject", "/tests"]
    }
}
