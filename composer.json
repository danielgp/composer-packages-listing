{
    "name": "danielgp/composer-packages-listing",
    "description": "Wrapper for the composer.lock files attempting to normalize and enhance content included",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Popiniuc",
            "email": "danielpopiniuc@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2 || ^7.3 || ^7.4 || ^8.0 || ^8.1 || ^8.2 || ^8.3 || ^8.4"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "~0.7",
        "phpunit/phpunit": "^9",
        "phpunit/php-code-coverage": "^9",
        "squizlabs/php_codesniffer": "^3",
        "phpcompatibility/php-compatibility": "^9"
    },
    "autoload": {
        "classmap": [
            "source/Basics.php",
            "source/ComposerPackagesListing.php"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "archive": {
        "exclude": ["/nbproject", "/tests"]
    }
}
